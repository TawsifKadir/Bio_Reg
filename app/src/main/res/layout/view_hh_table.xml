<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:layout_marginTop="8dp"
    tools:padding="0dp">


    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/border_table_root">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="@dimen/table_row_height_header">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/border_table_cell" />

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="3"
                android:orientation="vertical">

                <TextView
                    style="@style/AppTextView.Table.Header"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/table_row_height_header_half"
                    android:background="@drawable/border_table_cell"
                    android:text="@string/male" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/table_row_height_header_half"
                    android:orientation="horizontal">

                    <TextView
                        style="@style/AppTextView.Table.Header"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@drawable/border_table_cell"
                        android:text="@string/normal" />

                    <TextView
                        style="@style/AppTextView.Table.Header"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@drawable/border_table_cell"
                        android:text="@string/with_disability" />

                    <TextView
                        style="@style/AppTextView.Table.Header"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@drawable/border_table_cell"
                        android:text="@string/with_chronically_ill" />

                </LinearLayout>


            </LinearLayout>


            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="3"
                android:orientation="vertical">

                <TextView
                    style="@style/AppTextView.Table.Header"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/table_row_height_header_half"
                    android:background="@drawable/border_table_cell"
                    android:text="@string/female" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/table_row_height_header_half"
                    android:orientation="horizontal">

                    <TextView
                        style="@style/AppTextView.Table.Header"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@drawable/border_table_cell"
                        android:text="@string/normal" />

                    <TextView
                        style="@style/AppTextView.Table.Header"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@drawable/border_table_cell"
                        android:text="@string/with_disability" />

                    <TextView
                        style="@style/AppTextView.Table.Header"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@drawable/border_table_cell"
                        android:text="@string/with_chronically_ill" />

                </LinearLayout>


            </LinearLayout>

        </TableRow>

        <TableRow
